[Desktop Entry]
Name=Toggle Desktop
GenericName=Toggle Desktop
Comment=Show Desktop (wmctrl)
Exec=bash -c '[[ $(wmctrl -m | grep "mode: ON") ]] && wmctrl -k off || exec wmctrl -k on'
#Icon=preferences-desktop-display
#Icon=user-desktop
Icon=emblem-desktop
#Icon=gnome-fs-desktop
#Icon=preferences-desktop
#Icon=application-x-desktop
Terminal=false
Type=Application
Categories=GTK;Utility;System;
